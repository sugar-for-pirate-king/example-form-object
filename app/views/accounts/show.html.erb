<% flash.each do |key, val| %>
  <%= val %>
<% end %>

<h4>Information</h4>

<table>
  <tr>
    <td>Name</td>
    <td><%= current_user.name %></td>
  </tr>

  <tr>
    <td>Phone Number</td>
    <td><%= current_user.phone_number %></td>
  </tr>

  <tr>
    <td>Bank</td>
    <td><%= current_user.account.bank.name %></td>
  </tr>

  <tr>
    <td>Account Number</td>
    <td><%= current_user.account.number %></td>
  </tr>
</table>